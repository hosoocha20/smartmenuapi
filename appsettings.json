{
  "ConnectionStrings": {
    "DefaultConnection": "DefaultConnectionString"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtSettings": {
    "Issuer": "DefaultIssuer", // The issuer of the token (can be your application's name)
    "Audience": "DefaultAudience", // The expected recipient of the token (can be your API or client)
    "SecretKey": "DefaultSecretKey", // The secret key used to sign the token
    "ExpirationInMinutes": 60 // Optional: Expiration time in minutes for the token
  },
  "AllowedHosts": "*"
}
